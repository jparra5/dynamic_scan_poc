{
  "name": "apidoc",
  "version": "0.17.4",
  "description": "RESTful web API Documentation Generator",
  "author": {
    "name": "Peter Rottmann",
    "email": "rottmann@inveris.de"
  },
  "license": "MIT",
  "preferGlobal": true,
