    "winston": "~2.3.1"
  },
  "devDependencies": {
    "apidoc-example": "*",
    "jshint": "^2.9.4",
    "lodash-cli": "^4.17.4",
    "mocha": "~3.2.0",
    "npm-check-updates": "^2.8.9",
    "path-to-regexp": "^1.7.0",
    "semver": "^5.3.0",
    "should": "~11.1.2",
    "webfontloader": "^1.6.27"
  },
  "jshintConfig": {
    "camelcase": true,
    "curly": false,
    "eqeqeq": true,
    "forin": true,
    "latedef": false,
    "newcap": true,
    "undef": true,
    "unused": true,
    "trailing": true,
    "node": true,
    "browser": true,
    "predef": [
      "it",
      "describe",
      "before",
      "after"
    ]
